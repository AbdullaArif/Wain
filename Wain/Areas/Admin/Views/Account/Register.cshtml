@using Wain.ViewModels.Account;
@model RegisterVM
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Wain - Rent a Car</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/Formassets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/Formassets/vendors/base/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/Formassets/css/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="~/Formassets/images/favicon.png" />
</head>

<body>
    <div class="container-scroller">
        <!-- partial:../../partials/_navbar.html -->
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:../../partials/_sidebar.html -->
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="row " >

                        <div class="col-12 grid-margin stretch-card">
                            <div class="card">
                                <div class="card-body">
                                    <h4 class="card-title">Register</h4>
                                   
                                    <form class="forms-sample" method="post" enctype="multipart/form-data">
                                        <div class="col-md-6 col-12">
                                            <label asp-for="IsSeller">Is Seller?</label>
                                            <input asp-for="IsSeller" />
                                            <span asp-validation-for="IsSeller" class="text-danger"></span>
                                        </div><br/>
                                        <div class="col-md-6 col-12">
                                            <label asp-for="Name">Name</label>
                                            <input asp-for="Name" placeholder="Name" />
                                            <span asp-validation-for="Name" class="text-danger"></span>
                                        </div><br />
                                        <div class="col-md-6 col-12">
                                            <label asp-for="Surname">Surname</label>
                                            <input asp-for="Surname" placeholder="Surname" />
                                            <span asp-validation-for="Surname" class="text-danger"></span>
                                        </div><br />
                                        <div class="col-md-12">
                                            <label asp-for="Username">Username</label>
                                            <input asp-for="Username" placeholder="Username" />
                                            <span asp-validation-for="Username" class="text-danger"></span>
                                        </div><br />
                                        <div class="col-md-12">
                                            <label asp-for="Email">Email</label>
                                            <input asp-for="Email" placeholder="Email" />
                                            <span asp-validation-for="Email" class="text-danger"></span>
                                        </div><br />
                                        <div class="col-md-12">
                                            <label asp-for="Password">Password</label>
                                            <input asp-for="Password" placeholder="Password" />
                                            <span asp-validation-for="Password" class="text-danger"></span>
                                        </div><br />
                                        <div class="col-md-12">
                                            <label asp-for="ConfirmPassword">Confirm Password</label>
                                            <input asp-for="ConfirmPassword" placeholder="Confirm Password" />
                                            <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                                        </div><br />
                                        <div class="col-md-6 col-12">
                                            <label asp-for="Mobile">Mobile</label>
                                            <input asp-for="Mobile" placeholder="Mobile" />
                                            <span asp-validation-for="Mobile" class="text-danger"></span>
                                        </div><br />
                                        <div class="col-md-6 col-12">
                                            <label asp-for="Address">Address</label>
                                            <input asp-for="Address" placeholder="Address" />
                                            <span asp-validation-for="Address" class="text-danger"></span>
                                        </div><br />
                                        <div class="col-md-6 col-12">
                                            <label asp-for="Photo">Photo</label>
                                            <input asp-for="Photo" placeholder="Photo" />
                                            <span asp-validation-for="Photo" class="text-danger"></span>
                                        </div><br />
                                        <div style="color:red;" asp-validation-summary="ModelOnly">
                                        </div>
                                        <button type="submit" class="btn btn-primary me-2">Register</button>
                                        <a asp-area="" asp-controller="home" asp-action="index" class="btn btn-light">Cancel</a>
                                    </form>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:../../partials/_footer.html -->
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                    </div>
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/Formassets/vendors/base/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- inject:js -->
    <script src="~/Formassets/js/off-canvas.js"></script>
    <script src="~/Formassets/js/hoverable-collapse.js"></script>
    <script src="~/Formassets/js/template.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/Formassets/js/file-upload.js"></script>
    <!-- End custom js for this page-->
</body>

</html>





